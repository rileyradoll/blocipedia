<h2>My Wikis</h2>

<div class="col-md-12">
  <%= link_to "New Public Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>

<div class="row">
  <div class="col-md-12">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
